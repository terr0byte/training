{
  "usesTypeScript": true,
  "usesStyledComponents": false,
  "usesCssModule": true,
  "cssPreprocessor": "scss",
  "testLibrary": "Testing Library",
  "component": {
    "default": {
      "customTemplates": {
        "component": ".templates/component/TemplateName.tsx",
        "style": ".templates/component/TemplateName.module.scss",
        "story": ".templates/component/TemplateName.stories.tsx",
        "lazy": ".templates/component/index.tsx"
      },
      "path": "src/components",
      "withStyle": true,
      "withTest": false,
      "withStory": true,
      "withLazy": false
    },
    "page": {
      "customTemplates": {
        "component": ".templates/pages/TemplateName.tsx",
        "style": ".templates/pages/TemplateName.module.scss",
        "story": ".templates/pages/TemplateName.stories.tsx",
        "lazy": ".templates/pages/index.tsx"
      },
      "path": "src/routes",
      "withLazy": true,
      "withStory": true,
      "withStyle": true,
      "withTest": false
    }
  }
}